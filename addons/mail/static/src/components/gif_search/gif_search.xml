<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="mail.GifList" owl="1">
        <div class="o_GifList">
            <div class="grid" t-on-scroll="_onScroll">
                <t t-foreach="gifManager.searchGifs" t-as="gif" t-key="gif.localId">
                    <div class="grid-item">
                        <Gif gifLocalId="gif.localId" />
                    </div>
                </t>
            </div>
        </div>
    </t>

</templates>
