<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_inventory_conflict_form_view" model="ir.ui.view">
        <field name="name">stock.inventory.conflict.form.view</field>
        <field name="model">stock.inventory.conflict</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <form>
                <div>
                    <strong>Some moves were made since your last inventory count. Plese count the inventory quantity again.</strong>
                </div>
                <div>
                    <br/>
                    <field name="quant_to_fix_ids" style="margin-top:10px;" context="{'tree_view_ref': 'stock.view_stock_quant_tree_editable'}">
                    </field>
                </div>
                <footer>
                    <button name="cancel_button" string="Discard" class="btn-primary" special="cancel"/>
                    <button name="action_validate" string="Confirm" type="object" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
