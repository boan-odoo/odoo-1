<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_demo_company_gb" model="res.partner">
        <field name="name">GB Company</field>
        <field name="vat"></field>
        <field name="street"></field>
        <field name="city"></field>
        <field name="country_id" ref="base.gb"/>
        <field name="state_id" ref="base.state_gb119"/>
        <field name="zip"></field>
        <field name="phone">+32 010 12 34 56</field>
        <field name="email">info@company.gbexample.com</field>
        <field name="website">www.gbexample.com</field>
    </record>

    <record id="demo_company_gb" model="res.company">
        <field name="name">GB Company</field>
        <field name="partner_id" ref="partner_demo_company_gb"/>
    </record>

    <function model="res.company" name="_onchange_country_id">
        <value eval="[ref('demo_company_gb')]"/>
    </function>

    <function model="res.users" name="write">
        <value eval="[ref('base.user_root'), ref('base.user_admin'), ref('base.user_demo')]"/>
        <value eval="{'company_ids': [(4, ref('l10n_gb.demo_company_gb'))]}"/>
    </function>

    <function model="account.chart.template" name="try_loading">
        <value eval="[ref('l10n_gb.l10n_gb')]"/>
        <value model="res.company" eval="obj().env.ref('l10n_gb.demo_company_gb')"/>
    </function>
</odoo>
