<?xml version="1.0" encoding="UTF-8" ?>
<odoo><data>

    <!--EVENT BOOTH TYPE-->

    <record id="event_type_booths" model="event.type">
        <field name="name">Physical Booths</field>
        <field name="use_booth" eval="True"/>
    </record>

    <record id="event_type_booth_category_standard" model="event.type.booth.category">
        <field name="name">Standard Booth</field>
        <field name="event_type_id" ref="event_type_booths"/>
        <field name="image_1920" type="base64" file="event_booth/static/src/img/standard-booth.jpeg"/>
        <field name="is_multi_slots" eval="True"/>
        <field name="description" type="html">
            <div class="card text-center shadow-sm mx-4 mt32">
                <div class="card-header p-4 bg-600">
                    <h4 class="text-white m-0">Standard</h4>
                </div>
                <div class="card-body">
                    <h3>100 €</h3>
                </div>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item bg-600">
                        <span class="text-white">4m²</span>
                    </li>
                    <li class="list-group-item">
                        <span>1 Branded Booth</span>
                    </li>
                    <li class="list-group-item">
                        <span>Standard Location</span>
                    </li>
                    <li class="list-group-item">
                        <span>46" display screen</span>
                    </li>
                    <li class="list-group-item">
                        <span>1 desk</span>
                    </li>
                    <li class="list-group-item">
                        <span>Logo &amp; link on website</span>
                    </li>
                    <li class="list-group-item">
                        <span>Feature in agenda</span>
                    </li>
                    <li class="list-group-item">
                        <span>Promotional banners</span><br/>
                        <span>(for you emails &amp; website</span>
                    </li>
                    <li class="list-group-item">
                        <span>Invitation brochure</span>
                    </li>
                </ul>
            </div>
        </field>
    </record>

