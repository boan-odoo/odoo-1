<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">   
      
    <t t-name="wowl.CrashManager" owl="1">
        <div class="o_crash_manager">
          <t t-foreach="Object.values(crashes)" t-as="crash" t-key="crash.id">
              <t t-component="crash.dialogClass" error="crash.error" t-on-dialog-closed="onDialogClosed(crash.id)"/>
          </t>
      </div>
    </t>

</templates>
