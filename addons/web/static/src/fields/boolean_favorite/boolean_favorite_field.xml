<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.BooleanFavoriteField" owl="1">
        <div class="o_favorite" t-on-click.stop.prevent="">
            <a href="#">
                <i
                    class="fa"
                    role="img"
                    t-att-class="props.value ? 'fa-star' : 'fa-star-o'"
                    t-att-title="props.attrs.nolabel and label"
                    t-att-aria-label="label"
                /> <t t-if="!props.attrs.nolabel" t-esc="label" />
            </a>
        </div>
    </t>

</templates>
