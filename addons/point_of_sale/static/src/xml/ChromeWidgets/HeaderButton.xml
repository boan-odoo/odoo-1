<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="HeaderButton" owl="1">
        <div class="header-button" t-att-class="{ confirm: state.label === 'Confirm' }" t-on-click="onClick">
            <span><i class="fa fa-sign-out" role="img"/> <t t-if="!env.isMobile" t-esc="translatedLabel"/></span>
        </div>
    </t>

</templates>
