<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- URL of the merchant managing the adyen accounts in this instance -->
    <record id="merchant_url" model="ir.config_parameter">
        <field name="key">odoo_payments.merchant_url</field>
        <field name="value">https://www.odoo.com/odoo_adyen/</field>
    </record>

    <!-- URL of the proxy to which API requests must be sent -->
    <record id="proxy_url" model="ir.config_parameter">
        <field name="key">odoo_payments.proxy_url</field>
        <field name="value">https://payment-adyen.odoo.com/payment_proxy_adyen/</field>
    </record>

</odoo>
