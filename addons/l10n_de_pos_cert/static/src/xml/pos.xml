<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="PosTicket">
        <t t-jquery=".receipt-change" t-operation="after">
            <t t-if="'tss' in receipt">
                <t t-foreach="receipt.tss" t-as="info" t-key="info">
                    <div class="tss-info">
                        <t t-esc="info_value.name" />
                        <span t-esc='info_value.value' class="pos-receipt-right-align"/>
                    </div>
                </t>
            </t>
        </t>
    </t>
</templates>
